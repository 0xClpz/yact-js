{
  "name": "yact-js",
  "version": "0.1.8",
  "repository": "git@github.com:hwaterke/yact-js.git",
  "author": "Harold Waterkeyn",
  "license": "MIT",
  "bin": {
    "yact": "./build/index.js"
  },
  "scripts": {
    "prepublishOnly": "yarn build",
    "prebuild": "rm -r build || true",
    "build": "babel src --copy-files --out-dir build",
    "format": "prettier --single-quote --bracket-spacing=false --write 'src/**/*.js'",
    "precommit": "lint-staged"
  },
  "dependencies": {
    "babel-runtime": "^6.26.0",
    "chalk": "^2.1.0",
    "commander": "^2.11.0",
    "cross-spawn": "^5.1.0",
    "fs-extra": "^4.0.1",
    "lodash": "^4.17.4",
    "readline-sync": "^1.4.7"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-stage-3": "^6.24.1",
    "eslint": "^4.4.1",
    "eslint-plugin-import": "^2.7.0",
    "husky": "^0.14.3",
    "lint-staged": "^4.1.3",
    "prettier": "^1.6.1"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write --single-quote --bracket-spacing=false",
      "eslint --max-warnings 1",
      "git add"
    ]
  }
}
